#!/usr/bin/env php
<?php

require $_composer_autoload_path ?? __DIR__.'/../vendor/autoload.php';

$app = new Symfony\Component\Console\Application('Laravel Installer', Composer\InstalledVersions::getPrettyVersion('laravel/installer'));

if (method_exists($app, 'addCommand')) {
    $app->addCommand(new Laravel\Installer\Console\NewCommand);
} else {
    $app->add(new Laravel\Installer\Console\NewCommand);
}

$app->run();
